{
  "name": "videojs-record-pages",
  "private": true,
  "version": "1.0.0",
  "description": "videojs-record github pages",
  "homepage": "https://collab-project.github.io/videojs-record",
  "author": "Collab",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/collab-project/videojs-record/issues"
  },
  "main": "index.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "lint": "npm-run-all lint:*",
    "lint:html": "htmlhint examples index.html",
    "start": "npm-run-all -p start:* watch",
    "start:serve": "babel-node --presets=env scripts/server.js",
    "update": "npm-run-all -s update:*",
    "update:videojs-record": "cpx -v 'node_modules/videojs-record/dist/**/*' dist/",
    "update:videojs-wavesurfer": "cpx -v 'node_modules/videojs-wavesurfer/dist/**/*' dist/",
    "update:wavesurfer": "cpx -v 'node_modules/wavesurfer.js/dist/wavesurfer.min.*' dist/",
    "update:wavesurfer-microphone": "cpx -v 'node_modules/wavesurfer.js/dist/plugin/wavesurfer.microphone.min.*' dist/",
    "update:libvorbis": "cpx -v 'node_modules/libvorbis.js/js/libvorbis.min.js' examples/lib/libvorbis.js/",
    "update:lamejs": "cpx -v 'node_modules/lamejs/lame.*.js' examples/lib/lamejs/",
    "update:lamejs-worker": "cpx -v 'node_modules/lamejs/worker-example/*.js' examples/lib/lamejs/worker-example/",
    "update:opus": "cpx -v 'node_modules/opus-recorder/dist/*' examples/lib/opus-recorder/",
    "update:recorderjs": "cpx -v 'node_modules/recorderjs/dist/*.js' examples/lib/recorderjs/",
    "test": "echo No tests yet!",
    "watch": "npm-run-all -p watch:lint",
    "watch:lint": "nodemon --watch examples --exec npm run lint",
    "prepublish": "not-in-install && npm run build || in-install",
    "release:zip": "node scripts/release-zip.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/collab-project/videojs-record.git"
  },
  "dependencies": {
    "lamejs": "^1.2.0",
    "libvorbis.js": "^1.1.2",
    "opus-recorder": "^4.1.0",
    "recorderjs": "git+https://github.com/mattdiamond/Recorderjs.git",
    "video.js": "^6.6.3",
    "videojs-record": "^2.1.2",
    "videojs-wavesurfer": "^2.2.1",
    "webrtc-adapter": "^6.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "connect": "^3.6.6",
    "copy-dir": "^0.3.0",
    "cpx": "^1.5.0",
    "del": "^3.0.0",
    "download-tarball": "^1.1.0",
    "htmlhint": "^0.9.13",
    "in-publish": "^2.0.0",
    "mv": "^2.1.1",
    "nodemon": "^1.15.1",
    "npm-run-all": "^4.1.2",
    "portscanner": "^2.1.1",
    "serve-static": "^1.13.2",
    "zip-dir": "^1.0.2"
  }
}

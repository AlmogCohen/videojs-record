{
    "opts": {
        "template": "node_modules/tui-jsdoc-template",
        "encoding": "utf8",
        "destination": ".",
        "recurse": true,
        "package": "node_modules/videojs-record/package.json",
        "readme": "./index-content.html"
    },
    "templates": {
        "name": "videojs-record",
        "default": {
            "layoutFile": "./doc-layout.tmpl"
        },
        "css": [
            "styles/docs.css"
        ]
    },
    "plugins": ["plugins/markdown"]
}

